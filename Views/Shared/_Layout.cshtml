<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/css/Mystyle.css" rel="stylesheet" />
</head>
<body>
    <header>
        <h1>Logo</h1>
        <nav>
            <ul>
                <li>
                    @Html.ActionLink("About", "About", "Home")
                </li>
                <li>
                    @Html.ActionLink("Projects", "Projects", "Home")
                </li>
                <li>
                    <a asp-action="Contact" asp-controller="Home">Contact</a>
                </li>
            </ul>
        </nav>
    </header>
    <div class="container">
        @RenderBody()
    </div>


    <footer>
        <p> &copy; Haris Gusinac  @DateTime.Now.ToShortDateString()</p>
       
    </footer>
</body>
</html>
